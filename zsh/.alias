alias ohmyzsh="open ~/.oh-my-zsh"
alias config="mate ~/.zshrc"
alias reload="source ~/.zshrc"
alias crp="_create_new_repo"
alias ignore="_create_gitignore"
alias c="_compile_and_run_c"
alias myip="_myip"
alias chrome="open -na 'Google Chrome'"
alias chromen="open -na 'Google Chrome' --args --incognito"
alias for-printing="
	git -C ~/Desktop/for-printing pull origin master;
	git -C ~/Desktop/for-printing add -A;
	git -C ~/Desktop/for-printing commit -m 'Upload';
	git -C ~/Desktop/for-printing push origin master;
"
alias checkport="_checkport"
